{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    ":dependencyDashboard",
  ],
  enabledManagers: ["flux", "kubernetes", "helm-values", "github-actions"],
  flux: {
    fileMatch: ["(^|/)kubernetes/.+\\.yaml$"],
  },
  kubernetes: {
    fileMatch: ["(^|/)kubernetes/.+\\.yaml$"],
  },
  "helm-values": {
    fileMatch: ["(^|/)kubernetes/.+\\.yaml$"],
  },
}
